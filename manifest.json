{
    "manifest_version": 2,
    "name": "Random Password Generator",
    "version": "1.0",
    "description": "Generates random passwords.",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "browser_action": {
        "default_icon": {
            "48": "icons/48.png",
            "96": "icons/96.png"
        },
        "default_popup": "index.html"
    },
    "permissions": [
        "clipboardWrite"
    ]
}